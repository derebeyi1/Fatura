<footer id="footer" class=" navbar fixed-bottom navbar-light bg-light justify-content-center">
    <div class=" navbar fixed-bottom navbar-light bg-light justify-content-center">
        <div class="copyright">
            <div class="left">
                Copyright <strong><span>En Ucuz E-Fatura</span></strong>. Tüm Hakları Saklıdır.
            </div>
            <div class="right">
                @if (Session["kullanici"] != null && Session["kalanSure"] != null && (int)Session["kalanSure"] < 30)
                {

                    if ((int)Session["kalanSure"] < 10)
                    {
                        <span style="color:red;"> Lisans süreniz bitiyor.</span>
                        <span style="color:red;font-weight:bold">@Session["kalanSure"]</span>
                        <span style="color:red;"> gün kaldı.</span>
                    }
                    else
                    {
                        @:Lisans süreniz bitiyor.
                        @Session["kalanSure"] @:gün kaldı.
                    }
                }
            </div>
        </div>
    </div>
</footer><!-- End Footer -->

